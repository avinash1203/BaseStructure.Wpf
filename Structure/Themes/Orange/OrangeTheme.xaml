<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Structure.Themes.Orange"
                    xmlns:System="clr-namespace:System;assembly=mscorlib"
                    xmlns:Imaging="clr-namespace:System.Media;assembly=System"
                    >
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Themes/Common/CommonClrs.xaml" ></ResourceDictionary>
        <ResourceDictionary Source="DialogWindowTheme.xaml"  ></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
    <BitmapImage x:Key="CheckboxChecked" UriSource="../../Assets/Icons/OrangeTheme/Check_box2_Checked.png"></BitmapImage>
    <BitmapImage x:Key="CheckboxUnChecked" UriSource="../../Assets/Icons/OrangeTheme/Check_box_Unchecked.png"></BitmapImage>
    <SolidColorBrush x:Key="ManinBackgroundClr" Color="{StaticResource #FFFFFF}" />
    <SolidColorBrush x:Key="SeparatorClr" Color="{StaticResource #DCE0E4}" />
    <SolidColorBrush x:Key="LableClr" Color="{StaticResource #BBBCBF}" />
    <SolidColorBrush x:Key="ComboBoxBackgroundClr" Color="{StaticResource #Transparent}" />
    <SolidColorBrush x:Key="ComboBoxBorderBrushClr" Color="{StaticResource #DCE0E4}" />

    <SolidColorBrush x:Key="ComboBoxSelectedItemForegroundClr" Color="{StaticResource #787878}" />
</ResourceDictionary>